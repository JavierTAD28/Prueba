def factorial(n):
  #Numeros positivos con WHILE
  acum = 1
  cont = 1
  
  while acum<=n:
    cont = cont*i
    acum += 1
  print(cont)
    
def main():
  n = int(input("Dame el nÃºmero: "))
  if n<0:
    print("Factorial no definido para negativos")
  elif (n == 0) or (n == 1):
    print("El factorial es 1")
  else:
    factorial(n)
  

if __name__=='__main__':
  main()